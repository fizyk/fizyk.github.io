&lt;a id="rest_code_ec1ed470fd9a4fdc811e7c79c5fefdec-9" name="rest_code_ec1ed470fd9a4fdc811e7c79c5fefdec-9" href="https://fizyk.dev/pl/blog/python-i-wszystko-jest-obiektem/#rest_code_ec1ed470fd9a4fdc811e7c79c5fefdec-9"&gt;&lt;/a&gt;&lt;span class="o"&gt;&amp;gt;&amp;gt;&amp;gt;&lt;/span&gt; &lt;span class="nb"&gt;len&lt;/span&gt;&lt;span class="p"&gt;(&lt;/span&gt;&lt;span class="nb"&gt;dir&lt;/span&gt;&lt;span class="p"&gt;(&lt;/span&gt;&lt;span class="nb"&gt;dir&lt;/span&gt;&lt;span class="p"&gt;))&lt;/span&gt;
&lt;a id="rest_code_ec1ed470fd9a4fdc811e7c79c5fefdec-10" name="rest_code_ec1ed470fd9a4fdc811e7c79c5fefdec-10" href="https://fizyk.dev/pl/blog/python-i-wszystko-jest-obiektem/#rest_code_ec1ed470fd9a4fdc811e7c79c5fefdec-10"&gt;&lt;/a&gt;&lt;span class="mi"&gt;26&lt;/span&gt;
&lt;a id="rest_code_ec1ed470fd9a4fdc811e7c79c5fefdec-11" name="rest_code_ec1ed470fd9a4fdc811e7c79c5fefdec-11" href="https://fizyk.dev/pl/blog/python-i-wszystko-jest-obiektem/#rest_code_ec1ed470fd9a4fdc811e7c79c5fefdec-11"&gt;&lt;/a&gt;&lt;span class="o"&gt;&amp;gt;&amp;gt;&amp;gt;&lt;/span&gt; &lt;span class="nb"&gt;len&lt;/span&gt;&lt;span class="p"&gt;(&lt;/span&gt;&lt;span class="nb"&gt;dir&lt;/span&gt;&lt;span class="p"&gt;(&lt;/span&gt;&lt;span class="nb"&gt;len&lt;/span&gt;&lt;span class="p"&gt;))&lt;/span&gt;
&lt;a id="rest_code_ec1ed470fd9a4fdc811e7c79c5fefdec-12" name="rest_code_ec1ed470fd9a4fdc811e7c79c5fefdec-12" href="https://fizyk.dev/pl/blog/python-i-wszystko-jest-obiektem/#rest_code_ec1ed470fd9a4fdc811e7c79c5fefdec-12"&gt;&lt;/a&gt;&lt;span class="mi"&gt;26&lt;/span&gt;
